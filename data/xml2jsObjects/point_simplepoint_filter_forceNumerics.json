{
  "StyledLayerDescriptor": {
    "$": {
      "version": "1.0.0",
      "xsi:schemaLocation": "http://www.opengis.net/sld StyledLayerDescriptor.xsd",
      "xmlns": "http://www.opengis.net/sld",
      "xmlns:ogc": "http://www.opengis.net/ogc",
      "xmlns:xlink": "http://www.w3.org/1999/xlink",
      "xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance"
    },
    "NamedLayer": [{
      "Name": ["Simple Point Filter"],
      "UserStyle": [{
        "Title": ["Simple Point Filter"],
        "FeatureTypeStyle": [{
          "Rule": [{
            "Name": ["Small populated New Yorks"],
            "Filter": [{
              "$": {
                "xmlns": "http://www.opengis.net/ogc"
              },
              "And": [{
                "PropertyIsEqualTo": [{
                  "PropertyName": ["NAME"],
                  "Literal": ["New York"]
                }],
                "PropertyIsNull": [{
                  "PropertyName": ["TEST"]
                }],
                "PropertyIsLike": [{
                  "$": {
                    "wildCard": "*",
                    "singleChar": ".",
                    "escape": "!"
                  },
                  "PropertyName": ["TEST2"],
                  "Literal": ["*York*"]
                }, {
                  "$": {
                    "wildCard": "*",
                    "singleChar": ".",
                    "escape": "!"
                  },
                  "PropertyName": ["TEST1"],
                  "Literal": ["*New*"]
                }],
                "Not": [{
                  "PropertyIsGreaterThan": [{
                    "PropertyName": ["POPULATION"],
                    "Literal": ["100000"]
                  }]
                }],
                "Or": [{
                  "PropertyIsEqualTo": [{
                    "PropertyName": ["TEST2"],
                    "Literal": ["1"]
                  }, {
                    "PropertyName": ["TEST2_STR"],
                    "Literal": ["2"]
                  }]
                }]
              }]
            }],
            "MinScaleDenominator": ["10000"],
            "MaxScaleDenominator": ["20000"],
            "PointSymbolizer": [{
              "Graphic": [{
                "Mark": [{
                  "WellKnownName": ["circle"],
                  "Fill": [{
                    "CssParameter": [{
                      "_": "#FF0000",
                      "$": {
                        "name": "fill"
                      }
                    }]
                  }],
                  "Stroke": [{
                    "CssParameter": [{
                      "_": "#000000",
                      "$": {
                        "name": "stroke"
                      }
                    }, {
                      "_": "2",
                      "$": {
                        "name": "stroke-width"
                      }
                    }]
                  }]
                }],
                "Size": ["6"]
              }]
            }]
          }]
        }]
      }]
    }]
  }
}
